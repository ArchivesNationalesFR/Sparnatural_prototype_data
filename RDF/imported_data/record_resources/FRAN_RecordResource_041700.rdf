            <rico:title xml:lang="fr">Procès-verbal de refus. 1560, 18 avril - 1563, 12 avril</rico:title>
            <rdfs:label xml:lang="fr">Procès-verbal de refus. 1560, 18 avril - 1563, 12 avril</rdfs:label>
            <rico:beginningDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date">1560-04-18</rico:beginningDate>
            <rico:endDate rdf:datatype="http://www.w3.org/2001/XMLSchema#date">1563-04-12</rico:endDate>
